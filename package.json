{
  "name": "crew-performance-chatbot",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "sandbox": "npx ampx sandbox",
    "deploy:dev": "npx ampx pipeline-deploy --branch dev",
    "deploy:prod": "npx ampx pipeline-deploy --branch main",
    "generate-env": "node scripts/generate-env.js",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write src"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@aws-amplify/backend": "latest",
    "@aws-amplify/backend-cli": "latest",
    "aws-amplify": "^6.0.0",
    "@anthropic-ai/sdk": "^0.20.0",
    "pg": "^8.11.0",
    "react-markdown": "^9.0.0",
    "date-fns": "^3.0.0",
    "lucide-react": "^0.300.0",
    "recharts": "^2.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0"
